    "fetch-mock": "^5.8.1",
    "flux-standard-action": "^1.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "redux-devtools-extension": "^1.0.0",
    "redux-mock-store": "^1.2.1",
