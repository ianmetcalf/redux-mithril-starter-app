  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianmetcalf/redux-mithril-starter-app/issues"
  },
  "homepage": "https://github.com/ianmetcalf/redux-mithril-starter-app#readme",
  "dependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-mjsx": "^4.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-env": "^1.1.6",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.26.1",
    "debug": "^2.6.0",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "hashmark": "^4.1.0",
    "hbs": "^4.0.1",
    "imports-loader": "^0.7.0",
    "lodash": "^4.17.4",
    "mithril": "^0.2.5",
    "morgan": "^1.7.0",
    "node-fetch": "^1.6.3",
    "normalizr": "^3.0.2",
    "postcss-loader": "^1.2.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
